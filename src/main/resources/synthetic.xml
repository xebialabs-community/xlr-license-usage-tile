<synthetic xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns="http://www.xebialabs.com/deployit/synthetic"
           xsi:schemaLocation="http://www.xebialabs.com/deployit/synthetic synthetic.xsd">

    <type type="xlrelease.LicenseUsage" label="XL Release License Usage" extends="xlrelease.Tile" description="Shows license usage.">
        <property name="width" kind="integer" default="1"/>
        <property name="height" kind="integer" default="1"/>
        <property name="embedded" kind="boolean" default="true" hidden="true" required="false"/>

        <property name="uri" kind="string" default="licenseusage/index.html" hidden="true"/>
        <property name="title" kind="string" description="Name of the tile" default="License Usage"/>
        <property name="supportedScopes" kind="list_of_string" default="global" hidden="true"/>
    </type>
</synthetic>
